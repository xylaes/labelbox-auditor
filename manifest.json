{
  "manifest_version": 3,
  "name": "LabelBox Auditor",
  "version": "3.0",
  "description": "Automated LabelBox audits with Gemini 2.5 Pro, Auto-Fill, and Human-Simulation.",
  "permissions": ["sidePanel", "scripting", "activeTab", "storage", "tabs"],
  "host_permissions": ["<all_urls>"],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
