{
  "manifest_version": 3,
  "name": "LabelBox Auditor",
  "version": "1.1",
  "permissions": ["sidePanel", "activeTab", "scripting", "storage"],
  "host_permissions": [
    "<all_urls>"
  ],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}